<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 800">
  <defs>
    <linearGradient id="room" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#F5F5F4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E7E5E4;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="window" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#7DD3FC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0EA5E9;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Room background -->
  <rect width="1200" height="800" fill="url(#room)"/>
  
  <!-- Floor -->
  <rect y="600" width="1200" height="200" fill="#D6D3D1"/>
  
  <!-- Window -->
  <rect x="750" y="150" width="400" height="450" fill="url(#window)"/>
  <rect x="750" y="150" width="400" height="450" fill="none" stroke="#78716C" stroke-width="8"/>
  <line x1="950" y1="150" x2="950" y2="600" stroke="#78716C" stroke-width="8"/>
  <line x1="750" y1="375" x2="1150" y2="375" stroke="#78716C" stroke-width="8"/>
  
  <!-- Plants in window view -->
  <ellipse cx="850" cy="500" rx="40" ry="60" fill="#15803D" opacity="0.6"/>
  <ellipse cx="1050" cy="520" rx="50" ry="70" fill="#166534" opacity="0.6"/>
  
  <!-- Bed -->
  <rect x="100" y="450" width="500" height="280" rx="20" fill="#44403C"/>
  <rect x="100" y="450" width="500" height="100" rx="15" fill="#57534E"/>
  
  <!-- Pillows -->
  <rect x="140" y="460" width="80" height="60" rx="10" fill="#D6D3D1"/>
  <rect x="250" y="460" width="80" height="60" rx="10" fill="#D6D3D1"/>
  <rect x="360" y="460" width="80" height="60" rx="10" fill="#D6D3D1"/>
  <rect x="470" y="460" width="80" height="60" rx="10" fill="#D6D3D1"/>
  
  <!-- Side table -->
  <rect x="50" y="550" width="80" height="100" fill="#292524"/>
  
  <!-- Lamp -->
  <rect x="65" y="530" width="50" height="20" rx="5" fill="#FCD34D" opacity="0.7"/>
  <rect x="80" y="550" width="20" height="40" fill="#78716C"/>
  
  <!-- Text -->
  <text x="600" y="100" font-family="Arial, sans-serif" font-size="48" font-weight="bold" 
        fill="#292524" text-anchor="middle">Premium Suite</text>
</svg>
