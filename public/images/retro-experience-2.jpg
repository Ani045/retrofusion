<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 800">
  <defs>
    <linearGradient id="evening" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#1E293B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#475569;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="light" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#FCD34D;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#F59E0B;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Evening sky -->
  <rect width="1200" height="800" fill="url(#evening)"/>
  
  <!-- Villa structure -->
  <rect x="50" y="450" width="500" height="300" fill="#451A03" opacity="0.8"/>
  <polygon points="50,450 300,350 550,450" fill="#292524" opacity="0.9"/>
  
  <!-- Lounge deck -->
  <rect x="600" y="500" width="550" height="280" fill="#78350F"/>
  
  <!-- Furniture silhouettes -->
  <rect x="700" y="580" width="120" height="80" rx="10" fill="#57534E"/>
  <rect x="900" y="580" width="120" height="80" rx="10" fill="#57534E"/>
  <circle cx="760" cy="560" r="30" fill="#44403C"/>
  <circle cx="960" cy="560" r="30" fill="#44403C"/>
  
  <!-- Hanging lights -->
  <circle cx="750" cy="520" r="15" fill="url(#light)"/>
  <line x1="750" y1="505" x2="750" y2="480" stroke="#A8A29E" stroke-width="2"/>
  
  <circle cx="900" cy="520" r="15" fill="url(#light)"/>
  <line x1="900" y1="505" x2="900" y2="480" stroke="#A8A29E" stroke-width="2"/>
  
  <circle cx="1050" cy="520" r="15" fill="url(#light)"/>
  <line x1="1050" y1="505" x2="1050" y2="480" stroke="#A8A29E" stroke-width="2"/>
  
  <!-- Plants -->
  <ellipse cx="150" cy="700" rx="60" ry="80" fill="#166534"/>
  <ellipse cx="1050" cy="700" rx="60" ry="80" fill="#166534"/>
  
  <!-- Text -->
  <text x="600" y="100" font-family="Arial, sans-serif" font-size="48" font-weight="bold" 
        fill="white" text-anchor="middle" opacity="0.9">Luxury Lounge</text>
</svg>
